{
  "name": "foodprint",
  "version": "1.0.0",
  "description": "food supply chain on Blockchain for small scale farmers",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": "16.x",
    "npm": "7.x"
  },
  "scripts": {
    "bundle": "./node_modules/browserify/bin/cmd.js src/js/app.js | uglifyjs -mc  > src/js/bundle.js",
    "preinstall": "rm -rf node_modules/*/.git/",
    "test": "NODE_ENV=test mocha --timeout 30000 tests/api/*.js",
    "build-dev": "sequelize db:migrate --config ./dbconfig.json --env development",
    "build": "sequelize db:migrate --config ./dbconfig.json --env production",
    "heroku-postbuild": "npm run bundle",
    "dev": "nodemon server.js --host 0.0.0.0",
    "start": "node server.js",
    "format": "prettier --write 'README.md' 'server.js' 'utils/*.js' 'migrations/*.js' 'routes/*.js' 'models/*.js' 'dbxml/*.js' 'config/*.js'"
  },
  "author": "Julian Kanjere, FoodPrint Labs",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "latest",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "gulp": "latest",
    "gulp-autoprefixer": "latest",
    "gulp-clean-css": "latest",
    "gulp-concat": "latest",
    "gulp-minify-css": "latest",
    "gulp-notify": "latest",
    "gulp-plumber": "latest",
    "gulp-rename": "latest",
    "gulp-sass": "latest",
    "gulp-sourcemaps": "latest",
    "gulp-uglify": "latest",
    "mocha": "^9.2.2",
    "nodemon": "^1.19.1",
    "prettier": "2.5.1",
    "sequelize-auto": "^0.8.4",
    "sequelize-cli": "^6.3.0",
    "watchify": "^3.11.1"
  },
  "dependencies": {
    "algosdk": "^1.12.0",
    "axios": "^0.26.1",
    "body-parser": "^1.19.0",
    "browserify": "^16.3.0",
    "connect-ensure-login": "^0.1.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.16.2",
    "express-validator": "^6.2.0",
    "fs": "0.0.1-security",
    "heroku-ssl-redirect": "0.0.4",
    "jimp": "^0.6.4",
    "method-override": "^3.0.0",
    "moment": "^2.27.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "node-fetch": "^3.1.0",
    "nodemailer": "^6.4.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "qrcode": "^1.4.0",
    "sequelize": "^6.6.5",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0",
    "uglify-js": "^3.13.7",
    "uuid": "^3.3.3"
  }
}
